{%
  set classes = [
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}

 <div{{ attributes.addClass(classes) }}>
    {{ title_prefix }}
    {{ title }}
    {{ title_suffix }}

    {% if header %}
        {{ header }}
    {% endif %}

    {{ exposed }}
    {{ attachment_before }}
  
    {% if rows -%}  
        {{ rows }}
    {% elseif empty %}
        {{ empty }}
    {% endif %}
    
    {% if footer %}
        {{ footer }}
    {% endif %}
    
    {% if pager %}
    <div class="pager col-md-12">
        {{ pager }}
    </div>
    {% endif %}
    
    {{ attachment_after }}
    {{ more }}
   

    {{ feed_icons }}
 </div>