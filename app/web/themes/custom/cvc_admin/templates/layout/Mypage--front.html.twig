 <div class="wrapper">
    <nav class="main-header navbar navbar-expand navbar-white navbar-light">

      {{ page.header }}

    </nav

    <!-- Main Sidebar Container -->
    <aside class="main-sidebar sidebar-dark-primary elevation-4">
      
      <!-- Brand Logo -->
      <a href="{{ path('<front>') }}" class="brand-link">
        <img src="{{base_path~theme_path}}/img/AdminLTELogo.png" alt="{{ site_name|t }}" class="brand-image img-circle elevation-3" style="opacity: .8">
        <span class="brand-text font-weight-light">{{ site_name }}</span>
      </a>
      <!-- Sidebar -->
      <div class="sidebar">
        
        <!-- Sidebar Menu -->
          {{ page.primary_menu }}
      </div>

    </aside>

    <!-- Page Main -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <div class="content-header">
          <div class="container-fluid">
            <div class="row mb-2">
              <div class="col-sm-6">
                {% if title %} <h1 class="m-0">{{title}}</h1>{% endif %}
              </div>
              <div class="col-sm-6">
                <h1> {{ title }} </h1>
              <ol class="breadcrumb float-sm-right">
                {{ page.breadcrumb }}
              </ol>
              </div>
            </div>
          </div><!-- /.container-fluid -->
        </div>


        <div class="content" 
            <div class="container-fluid">
                {% if page.content %}
                    {{ page.content }}
                {% endif %} 
            </div><!-- /.container-fluid -->
        </div><!-- /.content -->
  </div> <!-- ./content-wrapper -->
    <!-- Control Sidebar -->
  <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
    <div class="p-3">
      <h5>Title</h5>
      <p>Sidebar content</p>
    </div>
  </aside>
  <!-- /.control-sidebar -->

        {% if page.footer %}
        <footer class="main-footer">  
          {{ page.footer }}
        </footer>
        {% endif %}
    
  </div>


